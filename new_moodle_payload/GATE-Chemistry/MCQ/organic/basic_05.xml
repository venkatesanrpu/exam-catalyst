    <answer fraction="100" format="html">
      <text><![CDATA[An alkene]]></text>
      <feedback format="html">
        <text><![CDATA[Correct!]]></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text><![CDATA[An organozinc reagent]]></text>
      <feedback format="html">
        <text><![CDATA[Wrong!]]></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text><![CDATA[A boronic acid]]></text>
      <feedback format="html">
        <text><![CDATA[Wrong!]]></text>
      </feedback>
    </answer>
  </question>
  <question type="multichoice">
    <name>
      <text><![CDATA[Which coupling classically requires a]]></text>
    </name>
    <questiontext format="html">
      <text><![CDATA[Which coupling classically requires a copper(I) co-catalyst to activate terminal alkynes?]]></text>
